        for(let i =1;i<=8;i=i+2){
            if(i>=4){
                break;
            }
            console.log(i)
        }
    </script>
<script>
for(let i =1;i<=8;i=i+1){
            if(i>=4){
                continue;
            }
            console.log(i)
        }
    </script>
