   <script>
    let W = parseInt(prompt("Enter total number of  days?"))
    let A = parseInt(prompt("Enter number of absent days?"))
    let P = W-A
    let AP = (P/W)*100
    if(AP<75){
        document.write("Not eligible to write exam")
    }else{
        document.write("Eligible to write exam")
    }
